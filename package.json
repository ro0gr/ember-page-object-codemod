{
  "name": "ember-page-object-codemod",
  "version": "0.1.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/ro0gr/ember-page-object-codemod"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "test": "codemod-cli test",
    "test:coverage": "codemod-cli test --coverage",
    "update-docs": "codemod-cli update-docs",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "bin": "./bin/cli.js",
  "keywords": [
    "codemod-cli"
  ],
  "dependencies": {
    "codemod-cli": "^2.1.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "prettier": "^1.0.0"
  },
  "engines": {
    "node": "8.* || 10.* || >= 12"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
